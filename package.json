{
  "name": "website",
  "description": "A beautiful and sweet webseite.",
  "license": "UNLICENSED",
  "private": true,
  "version": "0.0.0",
  "main": "npm run dev",
  "browserslist": "defaults",
  "scripts": {
    "build": "cross-env GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "dev": "npm-run-all --parallel gatsby cms",
    "gatsby": "gatsby develop",
    "serve": "gatsby serve",
    "kill": "kill-port 8000",
    "cms": "npx netlify-cms-proxy-server",
    "update": "npx npm-check-updates -u --timeout 60000 && npm install",
    "format": "eslint --fix --ext .js,.jsx src/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@headlessui/react": "^1.4.1",
    "@heroicons/react": "^1.0.5",
    "@tailwindcss/aspect-ratio": "^0.3.0",
    "@tailwindcss/forms": "^0.3.4",
    "@tailwindcss/line-clamp": "^0.2.2",
    "@tailwindcss/typography": "^0.4.1",
    "autoprefixer": "^10.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.1.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-webpack-plugin": "^3.1.0",
    "gatsby": "^4.1.0",
    "gatsby-plugin-eslint": "^4.0.1",
    "gatsby-plugin-image": "^2.1.0",
    "gatsby-plugin-netlify-cms": "^6.1.0",
    "gatsby-plugin-postcss": "^5.1.0",
    "gatsby-plugin-react-helmet": "^5.1.0",
    "gatsby-plugin-sharp": "^4.1.0",
    "gatsby-plugin-sitemap": "^5.1.0",
    "gatsby-remark-images": "^6.1.0",
    "gatsby-source-filesystem": "^4.1.0",
    "gatsby-transformer-remark": "^5.1.0",
    "gatsby-transformer-sharp": "^4.1.0",
    "gatsby-transformer-yaml": "^4.1.0",
    "kill-port": "^1.6.1",
    "netlify-cms-app": "^2.15.58",
    "netlify-cms-locales": "^1.37.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.11",
    "postcss-loader": "^6.2.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.4.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "sanitize-html": "^2.5.3",
    "tailwindcss": "^2.2.19"
  }
}
